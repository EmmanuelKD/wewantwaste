{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022"], // Use a supported lib
    "module": "ESNext",
    "skipLibCheck": true,

    "moduleResolution": "node", // Use a supported value
    "moduleDetection": "force",
    "noEmit": true,

    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true
  },
  "include": ["vite.config.ts"]
}